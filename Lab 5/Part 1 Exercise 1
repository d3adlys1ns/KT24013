#include <iostream>
#include <algorithm>
using namespace std;

int main(){
    const int length = 10;
    string Array[length] = {"3", "2", "4", "7", "30", "21", "5", "15", "19", "1"};
    sort(Array, Array+length);
    for (int i=0; i < length; i++) cout << Array[i] << " ";
    // As we can see from the output, the sort function is able to sort the array of strings.
    
    return 0;
}
